Aweme.js = requires-body=1,max-size=-1,script-path=https://raw.githubusercontent.com/NobyDa/Script/master/Surge/JS/Aweme.js,pattern=^https://[\s\S]*\/aweme/v1/(feed|aweme/post|follow/feed)/,type=http-response
