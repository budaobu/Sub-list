[Script]
# > NextDNS
// nextdns_linkedip.js = script-path=Scripts/nextdns_linkedip.js,event-name=network-changed,type=event
# > Speed up download app ios
dnspod = script-path=https://raw.githubusercontent.com/budaobu/Rules/master/Surge/Scripts/dnspod.js,script-update-interval=-1,type=dns
# > 91ShortVideo
# 91ShortVideo.js = script-path=https://raw.githubusercontent.com/NobyDa/Script/master/Surge/JS/91ShortVideo.js,pattern=^https?:\/\/.+\.(my10api|(.*91.*))\.(com|tips|app|xyz)(:\d{2,5})?\/api.php$,type=http-request
# > VUE
VUE.js = requires-body=1,max-size=0,script-path=https://raw.githubusercontent.com/NobyDa/Script/master/Surge/JS/VUE.js,pattern=https:\/\/api\.vuevideo\.net\/api\/v1\/(users\/.+\/profile|subtitle\/prepare),type=http-response
# > VSCO
vsco.js = requires-body=1,max-size=0,script-path=https://raw.githubusercontent.com/NobyDa/Script/master/QuantumultX/File/vsco.js,pattern=^https?:\/\/vsco\.co\/api\/subscriptions\/2.1\/user-subscriptions\/,type=http-response
# > WeChat
WeChat.js = script-path=https://raw.githubusercontent.com/NobyDa/Script/master/Surge/JS/WeChat.js,pattern=^https://mp\.weixin\.qq\.com/mp/getappmsgad,type=http-request
# > Youtube
// YouTube.js = script-path=https://raw.githubusercontent.com/NobyDa/Script/master/Surge/JS/YouTube.js,pattern=^https://[\s\S]*\.googlevideo\.com/.*&(oad|ctier),type=http-request
# > Aweme
Aweme.js = requires-body=1,max-size=-1,script-path=https://raw.githubusercontent.com/NobyDa/Script/master/Surge/JS/Aweme.js,pattern=^https://[\s\S]*\/aweme/v1/(feed|aweme/post|follow/feed)/,type=http-response
# > Display Netflix TV series and movie's IMDb ratings, Douban ratings, rotten tomato and country/region
# ios.prod.ftl.netflix.com
nf_rating.js = script-path=https://raw.githubusercontent.com/yichahucha/surge/master/nf_rating.js,pattern=^https?://ios\.prod\.ftl\.netflix\.com/iosui/user/.+path=%5B%22videos%22%2C%\d+%22%2C%22summary%22%5D,type=http-request
nf_rating.js = requires-body=1,script-path=https://raw.githubusercontent.com/yichahucha/surge/master/nf_rating.js,pattern=^https?://ios\.prod\.ftl\.netflix\.com/iosui/user/.+path=%5B%22videos%22%2C%\d+%22%2C%22summary%22%5D,type=http-response
# > Display commodity historical price - JD
jd_price.js = requires-body=1,script-path=https://raw.githubusercontent.com/yichahucha/surge/master/jd_price.js,pattern=^https?://api\.m\.jd\.com/client\.action\?functionId=(wareBusiness|serverConfig),type=http-response
# > Display commodity historical price - Taobao
# trade-acs.m.taobao.com
tb_price.js = requires-body=1,script-path=https://raw.githubusercontent.com/yichahucha/surge/master/tb_price.js,pattern=^http://amdc\.m\.taobao\.com/amdc/mobileDispatch,type=http-response
tb_price.js = requires-body=1,script-path=https://raw.githubusercontent.com/yichahucha/surge/master/tb_price.js,pattern=^https?://trade-acs\.m\.taobao\.com/gw/mtop\.taobao\.detail\.getdetail,type=http-response
# > 人人视频 解锁AI原画
rrtv.js = requires-body=1,max-size=0,script-path=https://raw.githubusercontent.com/nzw9314/QuantumultX/master/Script/rrtv.js,pattern=^https:\/\/api\.rr\.tv(\/user\/privilege\/list|\/ad\/getAll|\/rrtv-video\/v4plus\/season\/detail),type=http-response
# > Faild
falied = script-path=https://raw.githubusercontent.com/budaobu/Rules/master/Surge/Scripts/failed.js,type=rule