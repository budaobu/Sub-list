#!name = Cloudflare public DNS
#!desc = Cloudflare public DNS(https://developers.cloudflare.com/1.1.1.1/)

[General]
doh-format = wireformat
doh-follow-outboound-mode = true
# > DoH
dns-server = 1.1.1.1, 1.0.0.1, 2606:4700:4700::1111, 2606:4700:4700::1001
doh-server = https://cloudflare-dns.com/dns-query

[Rule]
# PROTOCOL,DOH,Proxy

[Host]
# > DNS
cloudflare-dns.com = server:1.1.1.1, 1.0.0.1, 2606:4700:4700::1111, 2606:4700:4700::1001
